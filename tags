CFLAGS	Makefile	/^CFLAGS := -Wall -Werror -Wfatal-errors$/;"	m
CLIENT	Makefile	/^CLIENT := client$/;"	m
CLIENT_DEP	Makefile	/^CLIENT_DEP := $(CLIENT_SRC:%.c=$(TEMP)\/%.d)$/;"	m
CLIENT_OBJ	Makefile	/^CLIENT_OBJ := $(CLIENT_SRC:%.c=$(TEMP)\/%.o)$/;"	m
CLIENT_SRC	Makefile	/^CLIENT_SRC := $(shell find $(CLIENT)\/src\/* -type f -name "*.c")$/;"	m
PROXY_H	common/proxy.h	8;"	d
