SERVER_DEP	Makefile	/^SERVER_DEP := $(SERVER_SRC:%.c=$(TEMP)\/%.d)$/;"	m
SERVER_OBJ	Makefile	/^SERVER_OBJ := $(SERVER_SRC:%.c=$(TEMP)\/%.o)$/;"	m
SERVER_SRC	Makefile	/^SERVER_SRC := $(shell find $(SERVER)\/src\/* -type f -name "*.c")$/;"	m
TEMP	Makefile	/^TEMP := build$/;"	m
